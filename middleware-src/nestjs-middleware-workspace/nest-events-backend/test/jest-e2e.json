{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "db/prisma-postgres-connector/(.*)": "<rootDir>/../libs/prisma-postgres-connector/src/$1",
    "db/prisma-postgres-connector": "<rootDir>/../libs/prisma-postgres-connector/src",
    "@stackdb/prisma-stackdb-connector/(.*)": "<rootDir>/../libs/prisma-stackdb-connector/src/$1",
    "@stackdb/prisma-stackdb-connector": "<rootDir>/../libs/prisma-stackdb-connector/src",
    "@stackboard/stack-utils/(.*)": "<rootDir>/../libs/stack-utils/src/$1",
    "@stackboard/stack-utils": "<rootDir>/../libs/stack-utils/src",
    "@stackboard/prisma-pg-connector/(.*)": "<rootDir>/../libs/prisma-pg-connector/src/$1",
    "@stackboard/prisma-pg-connector": "<rootDir>/../libs/prisma-pg-connector/src",
    "@stackboard/stack-db-service-util/(.*)": "<rootDir>/../libs/stack-db-service-util/src/$1",
    "@stackboard/stack-db-service-util": "<rootDir>/../libs/stack-db-service-util/src"
  }
}